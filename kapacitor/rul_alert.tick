// rul_alert.tick
stream
  |from()
      .database('telegraf')
      .measurement('gy521')
  |eval(lambda: "rul_estimate")
      .as('rul')
  |alert()
      .warn(lambda: "rul" < 100)
      .crit(lambda: "rul" < 50)
      .message('{{ .Level }}: RUL={{ index .Fields "rul" }}')
  |influxDBOut()
      .database('telegraf')
      .measurement('alerts')
